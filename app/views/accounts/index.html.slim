= render("header")
main
  = paginate @accounts
  table.table
    thead
      tr
        th Name
        th Balance
        th Tariff
        th Zone
    tbody
      - @accounts.each do |account|
        tr
          td = link_to account.name, account
          td = account.balance
          td = account.tariff_link
          td = account.zone_link
  = paginate @accounts
